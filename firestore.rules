
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and allow anyone to read and write to your database.
    // They are intended for development and testing purposes only.
    // For a production app, you should implement more secure rules.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
